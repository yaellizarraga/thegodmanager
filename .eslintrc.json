{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console":"warn",
        "quotes":"warn",
        "indent":"warn",
        "no-unused-vars":"warn",
        "space-in-parens":"warn",
        "comma-dangle":"warn",
        "no-trailing-spaces":"warn",
        "max-len":"off",
        "semi":"warn",
        "eol-last":"warn"
    }
}
